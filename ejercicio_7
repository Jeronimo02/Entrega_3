    private void calcularActionPerformed(java.awt.event.ActionEvent evt) {                                         
        double a, b;
        a = Double.parseDouble(valA.getText());
        b = Double.parseDouble(valB.getText());
        if (a>b) {
            salida.setText("A es mayor que B");
        }
        else if (a==b) {
            salida.setText("A es igual a B");
        }
        else {
            salida.setText("B es mayor que A");
        }
    }                                        

